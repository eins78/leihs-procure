{
  "name": "leihs-procurement-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.18",
    "apollo-boost": "^0.1.6",
    "bootstrap": "^4.1.1",
    "browser-info": "^1.2.0",
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "downshift": "^1.31.14",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "^1.3.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.0",
    "react-icons": "^2.2.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "reactstrap": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --fix src/{,**/}*.js",
    "ci:lint": "eslint src/{,**/}*.js",
    "test": "react-scripts test --env=jsdom"
  },
  "proxy": {
    "/graphql": {
      "target": "http://localhost:3003"
    }
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.8.2",
    "prettier": "^1.12.1",
    "prettier-eslint": "^8.2.1"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  },
  "eslintConfig": {
    "plugins": [
      "prettier",
      "react"
    ],
    "extends": [
      "react-app",
      "eslint:recommended",
      "prettier",
      "prettier/react",
      "prettier/standard",
      "plugin:react/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "react/prop-types": "off"
    }
  }
}
